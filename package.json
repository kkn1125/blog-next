{
  "name": "blog3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "set404": "mv out/404/index.html out/404.html && rm -rf out/404/",
    "build": "next build && npm run set404",
    "postbuild": "next-sitemap --config next-sitemap.config.js && touch out/.nojekyll",
    "build-log": "next build >> build-log.txt",
    "postbuild-log": "next-sitemap --config next-sitemap.config.js >> postbuild-log.txt",
    "start": "next start",
    "lint": "next lint",
    "deploy": "cd out/ && git init && git add -A && git commit -m \"add post and fixbugs\" && git push -f https://github.com/kkn1125/kkn1125.github.io.git master:gh-pages"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@mui/x-date-pickers": "^6.5.0",
    "@next/mdx": "^13.4.3",
    "@types/mdx": "^2.0.5",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "animate.css": "^4.1.1",
    "animejs": "^3.2.1",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "glob": "^10.2.6",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "next": "13.4.4",
    "next-mdx-remote": "^4.4.1",
    "next-sitemap": "^4.1.3",
    "openai": "^3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "reading-time": "^1.5.0",
    "sharp": "^0.32.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.7",
    "@types/aos": "^3.0.4",
    "@types/markdown-it": "^12.2.3",
    "@types/react-syntax-highlighter": "^15.5.7",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "replace-in-files-cli": "^2.2.0"
  }
